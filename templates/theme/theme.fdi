### {theme-name}.fdi

# FD UI THEME (5.1+)

# Highlighted item background (current item in the Results panel)
ListView.Highlight={highlight-back}
# Highlighted item text (current item in the Results panel)
ListView.HighlightText={highlight1}
# Highlighted item background (matching items when doing a search in the Outline panel, selected item in the Types Explorer)
TreeView.Highlight={highlight-back}
# Highlighted item text (matching items when doing a search in the Outline panel, selected item in the Types Explorer)
TreeView.HighlightText={highlight1}

# Flash log viewer panel
# Debug text (all text not matched by warning or error regexes)
FlashLogViewer.DebugColor={content4}
# Warning text (warnings, matched by the warning regex)
FlashLogViewer.WarningColor={orange-max}
# Error text (errors, matched by the error regex)
FlashLogViewer.ErrorColor={red-max}

# Output panel (see OutputPanel > HighlightMarkers setting)
# Info text (compile status)
OutputPanel.InfoColor={content2}
# Debug text (traces)
OutputPanel.DebugColor={content4}
# Compiler warning text
OutputPanel.WarningColor={orange-max}
# Compiler error text (including warning and error lines)
OutputPanel.ErrorColor={red-max}
# Fatal text ("Fatal" custom highlight marker)
OutputPanel.FatalColor={magenta-max}
# FD process start text (fdbuild start)
OutputPanel.ProcessStartColor={blue}
# FD process end text (fdbuild complete)
OutputPanel.ProcessEndColor={blue}
# Matching text background
OutputPanel.HighlightColor={blue-min}

# Data tree control (data views in Locals, Watch debugger panels)
# Item text if a watch can't be resolved (e.g. identifier is out of scope or misspelled)
DataTreeControl.GrayText={content1}
# Item text if there is an error evaluating a variable or a watch
DataTreeControl.ErrorText={red}
# Highlighted item text (current item)
DataTreeControl.HighlightText={highlight1}
# Table grid lines
DataTreeControl.LineColor={grid-line-color}

# Project tree view (tree view in Project panel)
# SWF/SWC internal node text (e.g. exploring inside of an SWC or an SWF)
ProjectTreeView.SubItemColor={content2}
# SWF/SWC node text if there is an issue reading an SWF/SWC
ProjectTreeView.ErrorColor={red}
# Input SWF node text (?)
ProjectTreeView.InputColor={yellow}

# Completion list, snippet list (press Ctrl-B in the editor to open the snippets list, Ctrl-Alt-Space or type "new " to see the class name completion list)
# Selected line text
CompletionList.SelectedTextColor={highlight1}
# Selected line background
CompletionList.SelectedBackColor={highlight-back}
# Package text
CompletionList.PackageColor={content2}

# Method call tip
# Selected argument text
MethodCallTip.SelectedFore={highlight1}
# Selected argument background
MethodCallTip.SelectedBack={blue-min}
# Additional info text
MethodCallTip.InfoColor={content2}

# Data grid view (data view in Breakpoints debugger panels)
# Table grid lines
DataGridView.LineColor={grid-line-color}

# Text boxes that appear in toolbars and panels (search text field in Quick find bar, search text field in Outline panel)
# Search field invitation text ("Search..." in the Outline panel)
ToolStripTextBoxControl.GrayText={content1}

# Quick find bar (Ctrl-F)
# Quick find bar background color if there are no matches found
QuickFind.ErrorBack={blue}

# FD UI THEME (5.0+)

# When applying theme colors to a control based on type, whether to apply colors of control's base type before applying for it's own type
#ThemeManager.UseInheritance=True

# Image filter applied to toolbar and project panel icons (Bright, Default, Dim, Dark, Darker, Black)
ImageManager.ImageSet={image-set}

# Document tabs height (Default, Large) (Large tabs are taller than Default)
VS2005DockPaneStrip.TabSize=Large

# Document tabs style (Default, Block, Rect) (Default/Block look like tabs, Rect is rectangular shape and narrower)
VS2005DockPaneStrip.TabStyle=Rect

# Auto-hide panel strip style (Default, Underlined) (Default is with icons, Underlined is plain underlined text and narrower)
VS2005AutoHideStrip.TabStyle=Underlined
# Auto-hide panel strip style underline (when using Underlined style)
VS2005AutoHideStrip.TabUnderlineColor={content1}

# Panel strip open behavior (Default, Hover, Click) (Default/Hover - mouse over to open a panel, Click - click to open a panel)
DockPanel.ShowAutoHideContentOn=Hover

# Editor scrollbars
# Thumb and buttons
ScrollBar.ForeColor={content1}
# Background
ScrollBar.BackColor={background2}
# Rectangular border
ScrollBar.BorderColor={background2}
# Thumb during dragging
ScrollBar.ActiveForeColor={content4}
# Disabled scrollbar background
ScrollBar.DisabledBackColor={background2}
# Disabled scrollbar border
ScrollBar.DisabledBorderColor={background2}
# Small line marking the position of the caret line on the vertical scrollbar
ScrollBar.CurrentPositionColor={scrollbar-current-pos}

# Link label (links in the Project panel when there's no project open)
# Link text
LinkLabel.LinkColor={blue}
# Active (pressed) link text
LinkLabel.ActiveLinkColor={red}
# Disabled link text
LinkLabel.DisabledLinkColor={violet}

# List/table column header (files list view in Files panel)
# Background
ColumnHeader.BackColor={background2}
# Text
ColumnHeader.TextColor={header-fore}
# Column separator and border (horizontal line on the bottom)
ColumnHeader.BorderColor={content2}

# Combo boxes that appear in toolbars and panels (Debug/Release build in main toolbar, path selector in Files panel)
# Border
ToolStripComboBoxControl.BorderColor={content2}
# Border when focused or mouse overed
ToolStripComboBoxControl.ActiveBorderColor={content4}

# Text boxes that appear in toolbars and panels (search text field in Quick find bar, search text field in Outline panel)
# Border
ToolStripTextBoxControl.BorderColor={content2}

# Horizontal line below panel toolbars (below search text field in Output panel, below icons toolbar in Project panel)
ToolStrip.BorderColor={content2}
# Rectangular border around the editor
DockWindow.BorderColor={content3}
# Rectangular border around panels (Output panel, Project panel)
DockPanelControl.BorderColor={content4}
# Main and context menus horizontal separator (any menu)
ToolStripSeparator.ForeColor={content1}

# Main and context menu
# Submenu arrow (arrow next to Tools > Flash Tools)
ToolStripItem.ArrowColor={content2}
# Menu item text
ToolStripItem.TextColor={content4}

# Dock panel caption (panel title bar, Output panel title bar containing "Output" header, dock and close buttons)
# Text when panel is focused (just opened the "Output" panel, or clicked on the docked "Output" panel)
VS2005DockPaneCaption.ActiveForeColor={caption-fore}
# Dock and close buttons when panel is focused
VS2005DockPaneCaption.ActiveImageColor={caption-fore}
# Dock and close buttons when panel is not focused (dock the "Output" panel then click on the editor)
VS2005DockPaneCaption.ImageColor={content4}
# Rectangular border around the caption (title bar)
VS2005DockPaneCaption.BorderColor={content4}

# Document tab bar (below the main toolbar, above the editor)
# Dock panel tab bar (tab bar that appears if several dock panels are grouped together using drag'n'drop, and auto-hide is disabled) - settings with the Tool prefix
# Rectangular border around the active tab and a horizontal line below the whole tab bar
VS2005DockPaneStrip.DocTabActiveBorder={content1}
# Active tab text (filename)
VS2005DockPaneStrip.DocTabActiveForeColor={blue}
# Border around all tabs (horizontal line above all tabs in the tab bar) and tab separator
VS2005DockPaneStrip.DocTabInactiveBorder={background1}
# Dock panel tab bar - rectangular border around the active tab and a horizontal line above the whole tab bar
VS2005DockPaneStrip.ToolActiveBorderColor={content4}
# Unused
#VS2005DockPaneStrip.ToolBorderColor={background1}
# Dock panel tab bar - active tab text
VS2005DockPaneStrip.ToolActiveForeColor={highlight1}
# Dock panel tab bar - horizontal line above the active tab
VS2005DockPaneStrip.ToolSeparatorColor={background1}
# Tabs list and close tab buttons (buttons at the right side of the tab bar)
VS2005DockPaneStrip.ImageColor={content4}

# Main and context menus
# Selected main menu item background ("File" menu when it's open)
ToolStripMenu.TitleBackColor={background1}
# Rectangular border around selected main menu item (except horizontal line below the item)
ToolStripMenu.TitleBorderColor={content4}
# Horizontal line below selected main menu item
ToolStripMenu.SeparatorColor={background1}
# Pressed drop-down list button background ("Recent projects" button in the main toolbar)
ToolStripMenu.DropDownBackColor={background1}
# Border around pressed drop-down list button ("Recent projects" button in the main toolbar)
ToolStripMenu.DropDownBorderColor={content4}
# Main and context menu icons margin separator (vertical line separating icons margin from menu items)
ToolStripMenu.MarginBorderColor={background1}
# Main and context menu icons margin background
ToolStripMenu.MarginBackColor={background1}
# Rectangular border around a menu
ToolStripMenu.BorderColor={content4}
# Menu background
ToolStripMenu.BackColor={background1}

# Project panel - text color for added library assets
# Library (include referenced classes)
ProjectTreeView.LibraryTextColor={blue}
# Included library (include completely)
ProjectTreeView.ExternalLibraryTextColor={green}
# External library (not included)
ProjectTreeView.IncludedLibraryTextColor={cyan}

